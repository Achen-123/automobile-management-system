<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.po.CarState" table="carstate">
    
        <id name="sid" type="java.lang.Integer">
            <column name="SID" /> <!-- 表中的字段名 -->
           <generator class="native" />  <!-- 主键 ：自增-->
        </id>
        
        <property name="located" type="java.lang.String">
            <column name="LOCATED" /><!-- 表中的字段名 -->
        </property>
        
        <property name="carcond" type="java.lang.String">
            <column name="CARCOND" /><!-- 表中的字段名 -->
        </property>
        
        <property name="carnumber" type="java.lang.String">
        	<column name="CARNUMBER" /> <!-- 表中的字段名 -->
        </property>
        
       
        
    </class>
</hibernate-mapping>
