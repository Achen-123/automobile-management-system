<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.po.Car" table="car">
    
        <id name="cid" type="java.lang.Integer">
            <column name="CID" /> <!-- 表中的字段名 -->
           <generator class="native" />  <!-- 主键 ：自增-->
        </id>
        
        <property name="carname" type="java.lang.String">
            <column name="CARNAME" /><!-- 表中的字段名 -->
        </property>
        
        <property name="carnumber" type="java.lang.String">
        	<column name="CARNUMBER" /> <!-- 表中的字段名 -->
        </property>
        <property name="carcolor" type="java.lang.String">
        	<column name="CARCOLOR" /> <!-- 表中的字段名 -->
        </property>
        
        <property name="carorder" type="java.lang.String">
        	<column name="CARORDER" /> <!-- 表中的字段名 -->
        </property>
        
    </class>
</hibernate-mapping>
