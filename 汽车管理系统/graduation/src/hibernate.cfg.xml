<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
		"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
        <property name="hibernate.connection.password">123456</property>
        <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/management</property>
        <property name="hibernate.connection.username">root</property>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        
        <mapping resource="com/po/Userinfo.hbm.xml"/>
        <mapping resource="com/po/State.hbm.xml" />
        <mapping resource="com/po/Car.hbm.xml"/>
        <mapping resource="com/po/CarState.hbm.xml"/>
        <mapping resource="com/po/Owners.hbm.xml"/>
        <mapping resource="com/po/Orders.hbm.xml"/>
    </session-factory>
</hibernate-configuration>
