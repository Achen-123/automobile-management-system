<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.po.Orders" table="orders">
    
        <id name="oid" type="java.lang.Integer">
            <column name="OID" /> <!-- 表中的字段名 -->
           <generator class="native" />  <!-- 主键 ：自增-->
        </id>
        
        <property name="carorder" type="java.lang.String">
            <column name="CARORDER" /><!-- 表中的字段名 -->
        </property>
        
        <property name="memberid" type="java.lang.String">
        	<column name="MEMBERID" /> <!-- 表中的字段名 -->
        </property>
        
    </class>
</hibernate-mapping>
